//  1.2-------------------------无氧阈法-------------------------1.2

rule "AerobicStrengh2"
	activation-group "exerciseStrength"
	salience -1
	when
		Patient()
		and
		$exerciseData : ExerciseCardiopulmonary(vo2Max > 1.0)
		and
		$exercisePlan : ExercisePlan()
	then
		System.out.println("6...有运动心肺功能检测无氧阈法=================================");
