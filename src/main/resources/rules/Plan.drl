	salience 0
	when
		$assessment : Assessment(rm >= 0)
		and
		Patient(patientType == Patient.ORDINARY_PATIENT && riskRank == Patient.HIGHRISK)
		and
		$exercisePlan : ExercisePlan()
	then
		System.out.println("6...阻抗训练强度=================================");
